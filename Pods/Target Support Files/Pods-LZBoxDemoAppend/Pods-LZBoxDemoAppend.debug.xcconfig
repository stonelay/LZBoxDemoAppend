FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/BDNewRepo/ios" "${PODS_ROOT}/BlocksKit/ios" "${PODS_ROOT}/MJRefresh/ios" "${PODS_ROOT}/YAML/ios"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ALLFoundation" "${PODS_ROOT}/Headers/Public/NewAppendRepo"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/ALLFoundation" "${PODS_CONFIGURATION_BUILD_DIR}/NewAppendRepo"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ALLFoundation" -l"NewAppendRepo" -framework "BDNewRepo" -framework "BlocksKit" -framework "CoreGraphics" -framework "Foundation" -framework "MJRefresh" -framework "MessageUI" -framework "QuartzCore" -framework "UIKit" -framework "YAML"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
